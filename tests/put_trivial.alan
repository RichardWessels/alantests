source put_trivial
begin
    put "==============================\n";
    put "|        String Output       |\n";
    put "==============================\n";
    put "------------------------------\n";
    put "Hello World!" . "\tTab Escape" . "\n";
    put "\\Backslash" . "\t \" Quote" . "\n";
    put "------------------------------\n";
    put "==============================\n";
    put "|  Arithmetic Expressions    |\n";
    put "==============================\n";
    put "------------------------------\n";
    put "Number output: [24]\n";
    put "------------------------------\n";
    put 24 . "\n";
    put "------------------------------\n";
    put "Addition: [25]\n";
    put "------------------------------\n";
    put 12 + 13 . "\n";
    put "------------------------------\n";
    put "Multiplication: [8]\n";
    put "------------------------------\n";
    put 2 * 4 . "\n";
    put "------------------------------\n";
    put "Division: [2]\n";
    put "------------------------------\n";
    put 4 / 2 . "\n";
    put "------------------------------\n";
    put "Remainder: [0] & [3]\n";
    put "------------------------------\n";
    put 20 rem 5 . "\t" . 15 rem 4 . "\n";
    put "------------------------------\n";
    put "Subtraction: [-3] & [6]\n";
    put "------------------------------\n";
    put 1 - 4 . "\t" . 12 - 6 . "\n";
    put "------------------------------\n";
    put "Negation [-4] & [13]\n";
    put "------------------------------\n";
    put -4 . "\t" . -(-13) ."\n";
