    put "Boolean And: [F] & [T] & [F]\n";
    put "------------------------------\n";
    put true and false . "\t" . true and true . "\t" . false and false . "\n";
    put "------------------------------\n";
    put "Boolean Or: [T] & [F] & [T]\n";
    put "------------------------------\n";
    put true or false . "\t" . false or false . "\t" . false or true . "\n";
    put "------------------------------\n";
    put "Boolean Not: [T] & [F] & [T]\n";
    put "------------------------------\n";
    put  not false . "\n" . not true . "\n" . not (false and true) . "\n";
    put "------------------------------\n";
    put "Boolean Equals: [T] & [F]\n";
    put "------------------------------\n";
    put 6 = 6 . "\n" . 2 = 4 . "\n";
    put "------------------------------\n";
    put "Boolean GrEqual: [T] & [F] & [T]\n";
    put "------------------------------\n";
    put 6 >= 4 . "\n" . 2 >= 4 . "\n" . 4 >= 4 . "\n";
    put "------------------------------\n";
    put "Boolean Greater: [F] & [T] & [F]\n";
    put "------------------------------\n";
    put 2 > 4 . "\n" . 6 > 4 . "\n" . 4 > 4 . "\n";
    put "------------------------------\n";
    put "Boolean LessEqual: [T] & [F] & [T]\n";
    put "------------------------------\n";
    put 2 <= 4 . "\n" . 6 <= 4 . "\n" . 4 <= 4 . "\n";
    put "------------------------------\n";
    put "Boolean Less: [F] & [T] & [F]\n";
    put "------------------------------\n";
    put 6 < 4 . "\n" . 2 < 4 . "\n" . 4 < 4 . "\n";
    put "------------------------------\n";
    put "Boolean Not Equal: [T] & [F] & [T]\n";
    put "------------------------------\n";
    put 6 <> 4 . "\n" . 4 <> 4 . "\n" . 2 <> 4 . "\n";
    put "------------------------------\n"
end
